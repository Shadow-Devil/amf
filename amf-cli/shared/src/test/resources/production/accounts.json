    "swagger":"2.0",
    "definitions":{
        "accounts-attributes":{
            "type":"object",
            "description":"Group of flags which enable certain features or special behaviors on an Account",
            "properties":{
                "viewsReportsDisabled":{
                    "type":"boolean",
                    "description":"Prevents the staff account from being included in the list for filters in reports"
                },
                "infrequentUser":{
                    "type":"boolean",
                    "description":"Flag that indicates if the staff account is considered an infrequent user seat, as opposed to a full seat."
                },
                "permanentlyDisabled":{
                    "type":"boolean",
                    "description":"Permanently disables the staff account (for example, when a staff member leaves your organization). Note: The staff account will continue to appear in the tree of the Staff Accounts management console if the Show Disabled setting is on, but you will not be able to clear the Permanently Disable check box if you edit the account. You can reuse the login name of a permanently disabled staff account for a different staff account."
                },
                "accountLocked":{
                    "type":"boolean",
                    "description":"Temporarily locks the staff account. You might choose to do this if, for example, a staff member takes a leave of absence. This will also be set automatically if a staff member has exceeded the number of invalid logins or has an expired password."
                },
                "forcePasswordChange":{
                    "type":"boolean",
                    "description":"Forces the staff member to change her password the next time she logs in. Note: This flag remains true until the staff member changes her password."
                },
                "virtualAccount":{
                    "type":"boolean",
                    "description":"Flag indicating if the account is a Virtual Account.  Read-only"
                },
                "passwordNeverExpires":{
                    "type":"boolean",
                    "description":"Disables password expiration. This will override any system-wide password expiration configurations."
                },
                "canModifyEmailSignature":{
                    "type":"boolean",
                    "description":"Enables the staff account to modify the email signature."
                },
                "staffAssignmentDisabled":{
                    "type":"boolean",
                    "description":"Prevents the staff account from being assigned to incidents, answers, opportunities, and tasks"
                }
            }
        },
