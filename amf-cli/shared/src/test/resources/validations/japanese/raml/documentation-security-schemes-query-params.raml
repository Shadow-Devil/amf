#%RAML 1.0
title: Dropbox API
version: 1
baseUri: https://api.dropbox.com/{version}
securitySchemes:
  oauth_2_0:
    type: OAuth 2.0
    describedBy:
      queryParameters:
        access_token:
          description: |
            有効なOAuth 2アクセストークンを送信するために使用されます。 「Authorization」
            ヘッダーと共に使用しないでください。
          type: string
    settings:
          authorizationUri: https://www.dropbox.com/1/oauth2/authorize
          accessTokenUri: https://api.dropbox.com/1/oauth2/token
          authorizationGrants: [ authorization_code, implicit ]