#%RAML 1.0
title: My API With Types
types:
  Person:
    type: object
    discriminator: idontexist
    properties:
      name: string
      kind: string
  Employee:
    type: Person
    discriminatorValue: employee
    properties:
      employeeId: string
  User:
    type: Person
    discriminatorValue: user
    properties:
      userId: string