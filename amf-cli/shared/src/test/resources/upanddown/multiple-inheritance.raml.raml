#%RAML 1.0
title: My API With Types
types:
  HasHome:
    type: object
    properties:
      homeAddress:
        type: string
  Cat:
    type: object
    properties:
      name:
        type: string
      color:
        type: string
  Dog:
    type: object
    properties:
      name:
        type: string
      fangs:
        type: string
  HomeAnimal:
    type:
      - HasHome
      - Dog | Cat
