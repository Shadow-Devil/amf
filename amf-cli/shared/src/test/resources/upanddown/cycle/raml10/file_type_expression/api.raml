#%RAML 1.0
title: file type
version: 1.0
protocols: HTTP
baseUri: myApi.com

types:
  wrappedFile:
    type: object
    properties:
      fileprop: file
      stringprop: string

/file:
  post:
    description: Retrieve a list of all the users
    body:
      application/octet-stream:
        type: file
    responses:
      200:
        body:
          application/json:
            type: wrappedFile